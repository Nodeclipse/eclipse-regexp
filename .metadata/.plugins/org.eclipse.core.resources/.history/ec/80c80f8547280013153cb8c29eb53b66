package org.eclipse.regexp;

public class InputResult {

	private final String input;
	private final boolean mathes;
	private final String replaceFirst;
	private final String replaceAll;
	private final boolean lookingAt;
	private final boolean find;

	public InputResult(final String input, final boolean mathes,
			final String replaceFirst, final String replaceAll,
			final boolean lookingAt, final boolean find) {
		this.input = input;
		this.mathes = mathes;
		this.replaceFirst = replaceFirst;
		this.replaceAll = replaceAll;
		this.lookingAt = lookingAt;
		this.find = find;
	}
}

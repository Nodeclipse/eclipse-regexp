package org.eclipse.regexp;

import org.eclipse.core.runtime.IStatus;

public class TestResult {

	private IStatus status;

	public TestResult() {
	}

	public void addInpuResult(final String input) {
	}

	void setStatus(final IStatus status) {
		this.status = status;
	}

	public boolean isOk() {
		return status.isOK();
	}
}
